import pytesseract
from PIL import Image
pytesseract.pytesseract.tesseract_cmd = 'C://Program Files (x86)/Tesseract-OCR/tesseract-ocr-w64-setup-v4.1.0.20190314.exe'
image = Image.open(r"C:\Users\angel\Desktop\CS 450\Family_Search_Image\Y_record-image_ (2).jpg")
code = pytesseract.image_to_string(image, lang='chi_sim')
print(code)
